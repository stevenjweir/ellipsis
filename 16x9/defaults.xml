<includes>
    <default type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <aspectratio>stretch</aspectratio>
    </default>
    <default type="label">
        <posx>0</posx>
        <posy>0</posy>
        <font>Default</font>
        <textcolor>Text</textcolor>
        <shadowcolor>-</shadowcolor>
        <align>left</align>
    </default>
    <default type="fadelabel">
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <font>Default</font>
        <textcolor>Text</textcolor>
        <shadowcolor>-</shadowcolor>
        <textoffsetx>0</textoffsetx>
        <scrollout>false</scrollout>
        <resetonlabelchange>false</resetonlabelchange>
    </default>
    <default type="button">
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <font>Default</font>
        <textcolor>Text</textcolor>
        <disabledcolor>DisabledText</disabledcolor>
        <shadowcolor>-</shadowcolor>
        <textoffsetx>22</textoffsetx>
        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
        <texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
        <align>left</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="togglebutton">
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <font>Default</font>
        <textcolor>Text</textcolor>
        <disabledcolor>DisabledText</disabledcolor>
        <shadowcolor>-</shadowcolor>
        <textoffsetx>22</textoffsetx>
        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
        <texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
        <alttexturenofocus border="4,5,6,8">controls\button_nf.png</alttexturenofocus>
        <alttexturefocus border="4,5,6,8">controls\button_f.png</alttexturefocus>
        <align>left</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="textbox">
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <font>Default</font>
        <textcolor>Text</textcolor>
        <shadowcolor>-</shadowcolor>
    </default>
    <default type="progress">
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <texturebg>controls\toast-progress-empty.png</texturebg>
        <lefttexture>-</lefttexture>
        <midtexture>controls\toast-progress-full.png</midtexture>
        <righttexture>-</righttexture>
        <overlaytexture>-</overlaytexture>
        <reveal>true</reveal>
    </default>
    <default type="spincontrolex">
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <font>Default</font>
        <textcolor>Text</textcolor>
        <disabledcolor>DisabledText</disabledcolor>
        <shadowcolor>-</shadowcolor>
        <texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
        <textureup>controls\sortup_nf.png</textureup>
        <texturedown>controls\sortdown_nf.png</texturedown>
        <textureupfocus>controls\sortup_f.png</textureupfocus>
        <texturedownfocus>controls\sortdown_f.png</texturedownfocus>
        <reverse>yes</reverse>
        <textoffsetx>22</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrol">
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <font>Default</font>
        <textcolor>Text</textcolor>
        <disabledcolor>DisabledText</disabledcolor>
        <shadowcolor>-</shadowcolor>
        <texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
        <textureup>controls\sortup_nf.png</textureup>
        <texturedown>controls\sortdown_nf.png</texturedown>
        <textureupfocus>controls\sortup_f.png</textureupfocus>
        <texturedownfocus>controls\sortdown_f.png</texturedownfocus>
        <subtype>text</subtype>
        <reverse>yes</reverse>
        <textoffsetx>22</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="radiobutton">
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <font>Default</font>
        <textcolor>Text</textcolor>
        <disabledcolor>DisabledText</disabledcolor>
        <shadowcolor>-</shadowcolor>
        <textoffsetx>22</textoffsetx>
        <texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
        <textureradiofocus>controls\radiobutton_f.png</textureradiofocus>
        <textureradionofocus>controls\radiobutton_nf.png</textureradionofocus>
        <align>left</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="slider">
        <controloffsetx>75</controloffsetx>
        <controloffsety>0</controloffsety>
    </default>
    <default type="sliderex">
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
        <sliderwidth>150</sliderwidth>
        <sliderheight>32</sliderheight>
        <texturesliderbar>controls\slider_back.png</texturesliderbar>
        <textureslidernib>controls\slider_nib.png</textureslidernib>
        <textureslidernibfocus>controls\slider_nib_focus.png</textureslidernibfocus>
        <font>Medium</font>
        <textcolor>Text</textcolor>
        <disabledcolor>DisabledText</disabledcolor>
        <aligny>top</aligny>
        <textoffsetx>22</textoffsetx>
        <textoffsety>14</textoffsety>
    </default>
    <default type="mover">
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
    </default>
    <default type="resize">
        <posx>0</posx>
        <posy>0</posy>
        <width>0</width>
        <height>0</height>
        <texturefocus border="4,5,6,8">controls\button_f.png</texturefocus>
        <texturenofocus border="4,5,6,8">controls\button_nf.png</texturenofocus>
    </default>
    <default type="scrollbar" id="60">
        <texturesliderbackground>controls\scrollbar_back.png</texturesliderbackground>
        <texturesliderbar>controls\scrollbar_bar_nf.png</texturesliderbar>
        <texturesliderbarfocus>controls\scrollbar_bar_f.png</texturesliderbarfocus>
        <textureslidernib>-</textureslidernib>
        <textureslidernibfocus>-</textureslidernibfocus>
        <showonepage>false</showonepage>
        <orientation>vertical</orientation>
    </default>
</includes>