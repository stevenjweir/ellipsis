<includes>
    <!-- Dock Common -->
    <include name="Dock_LayoutOSD">
        <focusposition>0</focusposition>
        <orientation>horizontal</orientation>
        <scrolltime>250</scrolltime>
        <itemlayout height="135" width="150">
            <control type="label">
                <posx>0</posx>
                <posy>0</posy>
                <width>150</width>
                <height>135</height>
                <font>Default</font>
                <align>center</align>
                <info>ListItem.label</info>
                <visible>false</visible>
            </control>
        </itemlayout>
        <focusedlayout height="135" width="150">
            <control type="label">
                <posx>0</posx>
                <posy>0</posy>
                <width>150</width>
                <height>135</height>
                <font>Default</font>
                <align>center</align>
                <info>ListItem.label</info>
                <visible>false</visible>
            </control>
        </focusedlayout>
    </include>
    <include name="Dock_TabOSD">
        <control type="image">
            <posx>-960</posx>
            <posy>23</posy>
            <width>1920</width>
            <height>200</height>
            <texture>ui\filter_visor.png</texture>
            <animation effect="slide" start="0,98" end="0" time="300" tween="sine" easing="out" reversible="false">WindowOpen</animation>
            <animation effect="slide" start="0" end="0,98" time="300" tween="sine" easing="in" reversible="false">WindowClose</animation>
        </control>
    </include>
    <include name="Dock_ElipsisOSD">
        <control type="image">
            <posx>-48</posx>
            <posy>0</posy>
            <width>96</width>
            <height>96</height>
            <texture>button_osd_dock.png</texture>
            <animation type="WindowOpen" condition="true" reversible="false">
                <effect type="zoom" start="150" center="auto" time="400" />
                <effect type="fade" end="100" time="600" />
            </animation>
            <animation type="WindowClose" condition="true" reversible="false">
                <effect type="zoom" end="150" center="auto" time="400" />
                <effect type="fade" end="0" time="600" />
            </animation>
        </control>
    </include>
    <include name="Dock_LabelOSD">
        <control type="image">
            <posx>-453</posx>
            <posy>-93</posy>
            <width>908</width>
            <height>72</height>
            <texture>filter_iconlabel.png</texture>
        </control>
        <control type="label">
            <description>Current Selection</description>
            <posx>-960</posx>
            <posy>-90</posy>
            <width>1920</width>
            <align>center</align>
            <label>[UPPERCASE]$INFO[Container(9500).ListItem.Label][/UPPERCASE]</label>
            <font>Selected_Item</font>
            <textcolor>TextSelectedItem</textcolor>
            <shadowcolor>TextShadow</shadowcolor>
            <visible>!Skin.HasSetting(Disable_Icon_Labels)</visible>
            <animation effect="fade" time="300">Visible</animation>
            <animation effect="fade" time="300">Hidden</animation>
            <animation effect="fade" time="300">WindowOpen</animation>
            <animation effect="fade" time="300">WindowClose</animation>
        </control>
    </include>
