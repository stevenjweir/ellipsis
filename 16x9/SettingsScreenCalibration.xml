<window id="11">
    <defaultcontrol>8</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <controls>
        <control type="videowindow" id="20">
            <description>videowindow</description>
            <posx>0</posx>
            <posy>0</posy>
            <width>1920</width>
            <height>1080</height>
        </control>
        <control type="mover" id="8">
            <description>top left mover</description>
            <posx>0</posx>
            <posy>0</posy>
            <texturefocus>calibrate\cal_TL_fo.png</texturefocus>
            <texturenofocus>calibrate\cal_TL.png</texturenofocus>
        </control>
        <control type="mover" id="9">
            <description>right bottom mover</description>
            <posx>1050</posx>
            <posy>750</posy>
            <texturefocus>calibrate\cal_BR_fo.png</texturefocus>
            <texturenofocus>calibrate\cal_BR.png</texturenofocus>
        </control>
        <control type="mover" id="10">
            <description>subtitle position mover</description>
            <posx>300</posx>
            <posy>750</posy>
            <texturefocus>calibrate\cal_SUB_fo.png</texturefocus>
            <texturenofocus>calibrate\cal_SUB.png</texturenofocus>
        </control>
        <control type="resize" id="11">
            <description>pixel aspect ratio box</description>
            <posx>0</posx>
            <posy>405</posy>
            <width>432</width>
            <height>432</height>
            <texturefocus>calibrate\cal_PR_fo.png</texturefocus>
            <texturenofocus>calibrate\cal_PR.png</texturenofocus>
        </control>
        <control type="group">
            <posx>360</posx>
            <posy>90</posy>
            <control type="image" id="1">
                <posx>0</posx>
                <posy>0</posy>
                <width>1200</width>
                <height>105</height>
                <texture border="10,10,10,10">menu\library\panel-light.png</texture>
            </control>
            <control type="label" id="2">
                <description>coordinates label</description>
                <posx>30</posx>
                <posy>15</posy>
                <width>1125</width>
                <align>center</align>
                <label>-</label>
                <font>Medium</font>
                <shadowcolor>TextShadow</shadowcolor>
            </control>
            <control type="label" id="3">
                <description>help information</description>
                <posx>30</posx>
                <posy>60</posy>
                <width>1125</width>
                <align>center</align>
                <label>-</label>
                <font>Medium</font>
                <shadowcolor>TextShadow</shadowcolor>
            </control>
        </control>
        <include>Debug</include>
    </controls>
</window>